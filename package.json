{
  "name": "afterlife2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:backend": "npm run build --prefix backend",
    "start:backend": "npm run start --prefix backend",
    "build:frontend": "tsc -b && vite build",
    "start:frontend": "vite preview --host --port 5173",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "concurrently \"npm run build:backend && npm run start:backend\" \"sleep 5 && npm run build:frontend && npm run start:frontend\"",
    "dev:all": "concurrently \"npm run dev --prefix backend\" \"sleep 5 && npm run dev --prefix .\""
  },
  "dependencies": {
    "@netlify/functions": "^2.8.1",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "buffer": "^6.0.3",
    "canvas-confetti": "^1.9.3",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "mongoose": "^8.5.1",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "rcon-client": "^4.2.4",
    "react": "^18.3.1",
    "react-avatar-editor": "^13.0.2",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "^18.3.1",
    "react-easy-crop": "^5.0.8",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.25.1",
    "serverless-http": "^3.2.0",
    "tailwindcss": "^3.4.6"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/canvas-confetti": "^1.6.4",
    "@types/connect-mongo": "^3.1.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.14.12",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.16",
    "@types/passport-discord": "^0.1.13",
    "@types/react": "^18.3.3",
    "@types/react-avatar-editor": "^13.0.2",
    "@types/react-dom": "^18.3.0",
    "@types/react-easy-crop": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
